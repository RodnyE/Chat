{
    "env": {
        "browser": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:css/all"
    ],
    
    "plugins": ["html", "css"],
    "overrides": [
       
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-undef": 0,
        "no-unused-vars": 0,
        "no-constant-condition": 0,
        "no-prototype-builtins": 0,
        "no-extra-semi": "warn"
    }
}